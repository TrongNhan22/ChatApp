@page
@model IndexModel
@{
    ViewData["Title"] = "Home";
}

<head>
    <link rel="stylesheet" href="./css/index.css"/>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>

<div class="container container-index">
    <form class="d-flex">
        <div class="d-inline-block mt-3">
            <img src="https://cdn-icons-png.flaticon.com/512/9131/9131529.png" alt="username" class="avt-img"/>
        </div>
        <div class="d-inline-block mx-2">
            <label for="inputThink" class="form-label"></label>
            <input  id="inputThink" class="form-control border rounded" placeholder="Bạn đang nghĩ gì?"/>
        </div>
    </form>
    <div class="post row border rounded m-4 d-flex flex-column">
        <div class="d-flex ">
            <img src="data:image/jpeg;base64," alt="avatar" class="avt-img"/>
            <h6 class="mt-4 mx-2">Tên người dùng</h6>
        </div>
        <div class="title d-flex flex-column">
            <h4 class="heading text-black">Ngày đầu tiên đi học</h4>
            <p class="detail">Văn bản mô tả</p>
        </div>
        <div class="des ">
            <img src="https://media.baobinhphuoc.com.vn/upload/news/8_2023/23_1_09160322082023.png" alt="Ngày đầu tiên đi học" class="w-50" />
        </div>
        
        <div class="cmt d-flex justify-content-between border-top"> 
            <div class="feel d-flex">
                <span class="quantity-feel mt-2 mb-2 text-decoration-underline">200K</span>
                <div class="icon-feel border rounded m-2">
                    <i class="fa-solid icon-heart rounded mx-1 border fa-heart "></i>
                    <i class="fa-solid icon-smile rounded mx-1 border fa-face-smile "></i>
                    <i class="fa-solid icon-sad rounded mx-1 border fa-face-sad-tear "></i>
                </div>
            </div>
            <div class="d-flex">
                <span class="quantity-feel mt-2 mb-2 text-decoration-underline">20K</span>
